  "main": "src/main.ts",
  "scripts": {
    "postinstall": "bun -e \"await Bun.file('.environment').exists() || await Bun.write('.environment', Bun.file('.env.example'))\"",
    "commit": "cd .. && bun run commit",
    "development": "expo start",
    "dev": "bun run development",
    "build:dev": "eas build -e development -p android",
    "build:stage": "eas build -e preview -p android",
    "build": "eas build -p android",
    "lint": "eslint --fix src/ eslint.config.mjs app.config.ts"
  },
  "dependencies": {
    "@expo-google-fonts/nunito": "~0.3.0",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/stack": "^7.3.2",
    "axios": "^1.9.0",
    "expo": "^53.0.9",
    "expo-build-properties": "~0.14.6",
    "expo-dev-client": "~5.1.8",
    "expo-font": "~13.3.1",
    "expo-image-picker": "~16.1.4",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "~1.23.8",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@types/react": "~19.0.14",
    "eslint": "^9.27.0",
    "eslint-config-any": "^1.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
