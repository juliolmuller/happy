{
  "name": "happy-server",
  "version": "1.1.1",
  "private": true,
  "license": "MIT",
  "author": "Julio L. Muller",
  "scripts": {
    "typeorm": "tsnd node_modules/typeorm/cli.js",
    "migration": "npm run typeorm -- migration:run",
    "migration:fresh": "npm run typeorm -- schema:drop && npm run migration",
    "start": "tsnd src/index.ts --clear --respawn --transpile-only --ignore node_modules",
    "build": "rm -rf build/ && tsc -p ."
  },
  "dependencies": {
    "aws-sdk": "^2.818.0",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.1.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pg": "^8.5.1",
    "typeorm": "~0.2.29",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@lacussoft/eslint-config": "^1.0.1",
    "@types/cors": "^2.8.9",
    "@types/dotenv-flow": "^3.1.0",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/multer-s3": "^2.7.9",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
