{
  "name": "happy-server",
  "version": "1.1.3",
  "private": true,
  "license": "MIT",
  "author": "Julio L. Muller",
  "scripts": {
    "postinstall": "node -e \"require('fs').existsSync('.env') || require('fs').copyFileSync('.env.example', '.env')\"",
    "typeorm:js": "node node_modules/typeorm/cli.js",
    "typeorm": "tsnd node_modules/typeorm/cli.js",
    "seed": "tsnd src/database/seeds/",
    "migrate": "npm run typeorm -- migration:run",
    "migrate:seed": "npm run migrate && npm run seed",
    "migrate:reset": "npm run typeorm -- schema:drop && npm run migrate",
    "migrate:reset:seed": "npm run migrate:reset && npm run seed",
    "dev": "tsnd src/ --clear --respawn --transpile-only --ignore node_modules",
    "development": "npm run dev",
    "build": "rm -rf build/ && tsc -p .",
    "prod": "node build/",
    "production": "npm run prod",
    "lint": "eslint src/ --fix",
    "prestart": "npm run typeorm:js -- schema:drop && npm run typeorm:js -- migration:run && node build/database/seeds/",
    "start": "npm run prod"
  },
  "dependencies": {
    "aws-sdk": "^2.824.0",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.1.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pg": "^8.5.1",
    "typeorm": "~0.2.29",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@lacussoft/eslint-config": "^1.0.1",
    "@types/cors": "^2.8.9",
    "@types/dotenv-flow": "^3.1.0",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/multer-s3": "^2.7.9",
    "@types/pg": "^7.14.7",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
